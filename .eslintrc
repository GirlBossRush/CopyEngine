{
  "env": {
    "browser": true,
    "es6": true,
  },
  "extends": "eslint:recommended",
  "ecmaVersion": 7,
  "globals": {
    "Eager": false,
    "INSTALL_OPTIONS": false,
    "INSTALL_SCOPE": false,
  },
  "parser": "babel-eslint",
  "plugins": [
    "babel",
  ],
  "rules": {
    "babel/object-curly-spacing": [1, "never"],
    "array-bracket-spacing": [1, "never"],
    "arrow-parens": [1, "as-needed"],
    "arrow-spacing": [1, {
      "before": true,
      "after": true
    }],
    "block-spacing": [2, "always"],
    "brace-style": [1, "stroustrup", { "allowSingleLine": true }],
    "camelcase": 0,
    "comma-dangle": [2, "never"],
    "comma-spacing": [2, {
      "before": false,
      "after": true
    }],
    "comma-style": [1, "last"],
    "computed-property-spacing": [1, "never"],
    "consistent-this": [2, "self"],
    "consistent-return": 0,
    "constructor-super": 2,
    "curly": 0,
    "default-case": 1,
    "dot-location": [2, "property"],
    "dot-notation": 1,
    "eol-last": 1,
    "eqeqeq": [2, "allow-null"],
    "indent": [2, 2, {
      "SwitchCase": 1,
      "VariableDeclarator": 1
    }],
    "key-spacing": [1, {
      "afterColon": true,
      "beforeColon": false,
      "mode": "strict"
    }],
    "keyword-spacing": [1],
    "jsx-quotes": [1, "prefer-double"],
    "linebreak-style": [2, "unix"],
    "max-nested-callbacks": [1, 6],
    "new-cap": 2,
    "no-cond-assign": [2, "except-parens"],
    "new-parens": 2,
    "newline-after-var": 1,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-class-assign": 2,
    "no-console": 0,
    "no-const-assign": 2,
    "no-constant-condition": 1,
    "no-continue": 1,
    "no-debugger": 1,
    "no-delete-var": 2,
    "no-dupe-class-members": 2,
    "no-else-return": 1,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 1,
    "no-extra-parens": [1, "all"],
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 0,
    "no-labels": 1,
    "no-label-var": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 1,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-spaces": 1,
    "no-multiple-empty-lines": 1,
    "no-native-reassign": 2,
    "no-nested-ternary": 1,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-restricted-syntax": [2,
      "WithStatement"
    ],
    "no-redeclare": 2,
    "no-return-assign": 0,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 1,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unexpected-multiline": 2,
    "no-unused-vars": 1,
    "no-use-before-define": 2,
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-var": 2,
    "no-void": 1,
    "no-with": 2,
    "object-shorthand": [1, "always"],
    "one-var": [1, {
      "let": "never",
      "const": "never"
    }],
    "operator-assignment": [1, "always"],
    "operator-linebreak": [1, "after"],
    "padded-blocks": [1, "never"],
    "prefer-arrow-callback": 1,
    "prefer-const": 2,
    "prefer-reflect": 0,
    "prefer-spread": 1,
    "quote-props": [1, "as-needed", {
      "keywords": true
    }],
    "quotes": [1, "double"],
    "semi": [2, "never"],
    "semi-spacing": [1, {
      "after": true,
      "before": false
    }],
    "space-in-parens": [1, "never"],
    "space-infix-ops": [1],
    "space-unary-ops": [1],
    "spaced-comment": [1, "always"],
    "yoda": [1, "never", {
      "exceptRange": true
    }]
  }
}
